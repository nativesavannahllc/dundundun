<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nativesavannah.dundundun" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DunDunDun</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <engine name="android" version="3.7.2" />
    <feature name="Media">
        <param name="id" value="cordova-plugin-media" />
    </feature>
    <feature name="InAppBrowser">
        <param name="id" value="cordova-plugin-inappbrowser" />
    </feature>
</widget>
